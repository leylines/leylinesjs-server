  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/leylines/leylinesjs-server/issues"
  },
  "homepage": "https://github.com/leylines/leylinesjs-server",
  "dependencies": {
    "base-x": "^3.0.5",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.15.0",
    "compression": "^1.6.0",
    "cors": "^2.7.1",
    "express": "^4.8.0",
    "express-brute": "^1.0.1",
    "geodesy": "^2.4.0",
    "json5": "^2.1.0",
    "leylines-solids": "https://github.com/leylines/leylines-solids#main",
    "morgan": "^1.7.0",
    "pg": "^8.11.3",
    "proj4": "^2.3.12",
    "proj4js-defs": "0.0.1",
    "range_check": "^1.4.0",
    "request": "^2.67.0",
    "request-promise": "^4.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "jasmine": "^3.4.0",
    "supertest": "^4.0.0"
  }
